{
    "name": "kamil-koscielniak/eloquent-filters",
    "type": "library",
    "description": "Simply way to add filters to Eloquent models",
    "license": "MIT",
    "authors": [
        {
            "name": "kamil-koscielniak",
            "email": "koscielniak.k88@gmail.com"
        }
    ],
    "keywords": [
        "kamil-koscielniak",
        "eloquent-filters",
        "laravel",
        "eloquent",
        "filters"
    ],
    "require": {
        "php" : "^7.4",
        "illuminate/http": "^6.0|^7.0",
        "illuminate/support" : "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "KamilKoscielniak\\EloquentFilters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KamilKoscielniak\\EloquentFilters\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KamilKoscielniak\\EloquentFilters\\EloquentFiltersProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
